# Anthropic API key for Claude Haiku NLP query parsing
ANTHROPIC_API_KEY=

# Duffel API token for flight/hotel search (get from duffel.com/dashboard)
DUFFEL_API_TOKEN=

# OpenAI API key for semantic embeddings (optional — search degrades to keyword matching without it)
OPENAI_API_KEY=

# Supabase project URL and service role key (get from supabase.com/dashboard → Settings → API)
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# Upstash Redis — rate limiting + price intelligence (get from console.upstash.com)
# Required for: API rate limiting, price history, market trends, deal confidence scoring
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Amadeus Self-Service API (optional — hotel search uses estimated data without it)
AMADEUS_API_KEY=
AMADEUS_API_SECRET=
